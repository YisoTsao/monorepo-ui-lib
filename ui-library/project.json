{
  "name": "ui-library",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "ui-library/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/ui-library",
        "tsConfig": "ui-library/tsconfig.lib.json",
        "project": "ui-library/package.json",
        "entryFile": "ui-library/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "swc",
        "assets": [
          {
            "glob": "ui-library/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    }
  }
}
